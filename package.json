{
  "name": "uos-abreast-of-health-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.20.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "flow-bin": "^0.136.0",
    "flow-typed": "^3.2.1",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^5.0.0",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "17.0.0",
    "react-app-polyfill": "^1.0.6",
    "react-cookie": "^4.0.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "17.0.0",
    "react-focus-lock": "^2.5.0",
    "react-ga": "^3.2.1",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.1",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.4",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "sanitize.css": "^12.0.1",
    "superagent": "^5.3.1",
    "url-search-params-polyfill": "^8.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "flow": "flow",
    "flow:update-lib-types": "flow-typed install --ignoreDeps dev",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --verbose --coverage",
    "eject": "react-scripts eject",
    "version": "echo $npm_package_name@$npm_package_version",
    "lint": "node_modules/.bin/eslint ./src/ --max-warnings=0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "testing-library": "^0.0.2"
  }
}
